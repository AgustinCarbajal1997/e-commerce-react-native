{
  "expo": {
    "name": "final-project-react-native",
    "slug": "final-project-react-native",
    "version": "1.0.0",
    "platforms":["ios","android"],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier":"com.coderhouse.finalproject",
      "buildNumber":"1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package":"com.coderhouse.finalproject",
      "versionCode":1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions" : ["ACCESS_FINE_LOCATION",
      "WRITE_EXTERNAL_STORAGE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
